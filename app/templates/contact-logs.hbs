<div class="table-full">
    <div class="hr-divider m-t">
        <h3 class="hr-divider-content hr-divider-heading">Contact Logs</h3>
    </div>
    <div class="table-responsive">
        <table class="table" data-sort="table">
            <thead>
            <tr>
                <th class="header">Date</th>
                <th class="header">reference</th>
                <th class="header">Topic</th>
            </tr>
            </thead>
            <tbody>
            {{#each contact.contactLogs as |contactLogs|}}
                <tr>
                    <td>{{moment-format contactLogs.date}}</td>
                    <td>{{contactLogs.reference}}</td>
                    <td>{{contactLogs.topic}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
</div>
