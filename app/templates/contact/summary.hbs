<dl>
  <dt>Reference:</dt>
  <dd>{{model.reference}}&nbsp;</dd>
  <dt>Name:</dt>
  <dd>{{model.name}}&nbsp;{{link-to '(Link to Client)' 'contact' model}}&nbsp;</dd>
  {{#if model.mttbStepUncontactable}}
    <dt>Step Uncontactable:</dt>
    <dd><strong>{{model.mttbStepUncontactable}}</strong>&nbsp;</dd>
  {{/if}}
  <dt>Email:</dt>
  <dd><a target="_bank" href="mailto:{{model.email}}">{{model.email}}&nbsp;</a></dd>
  <dt>Home Phone:</dt>
  <dd>{{model.homePhone}}&nbsp;</dd>
  <dt>Cell Phone:</dt>
  <dd>{{model.cellPhone}}&nbsp;</dd>
  <dt>Office Phone:</dt>
  <dd>{{model.officePhone}}&nbsp;</dd>
  <dt>Skype ID:</dt>
  <dd>{{model.skypeId}}&nbsp; <a href="skype:{{model.skypeId}}?call">Call </a>|<a href="skype:{{model.skypeId}}?chat"> Chat</a> </dd>
  <dt>Address:</dt>
  <dd>{{model.address}}&nbsp;</dd>
  <dt>Address2:</dt>
  <dd>{{model.address_2}}&nbsp;</dd>
  <dt>City:</dt>
  <dd>{{model.city}}&nbsp;</dd>
  <dt>ZipCode/PostalCode</dt>
  <dd>{{model.zipCode}}&nbsp;</dd>
  <dt>State:</dt>
  <dd>{{model.state}}({{model.state_code}})&nbsp;</dd>
  <dt>Country:</dt>
  <dd>{{model.country}}({{model.country_code}})&nbsp;</dd>
  <dt>Time Zone:</dt>
  <dd>{{model.timeZone}}&nbsp;</dd>
  <dt>Last Contact:</dt>
  <dd>{{moment-format model.contactedAt}}&nbsp;</dd>
</dl>
<div class="hr-divider m-y">
  <h3 class="hr-divider-content hr-divider-heading">Agreement&nbsp;</h3>
</div>
<ul>
  {{#each-in (or model.agreementTags model.contact.agreementTags) as |key value|}}
    <li>{{key}}: <strong>{{value}}</strong></li>
  {{else}}
    <p class="text-center text-muted">No Agreements</p>
  {{/each-in}}

  {{#if (or model.silverAgreementSigned model.contact.silverAgreementSigned)}}
    <li> Silver Agreement Signed: {{moment-format (or model.mlrDate model.contact.mlrDate)}}</li>
  {{/if}}
</ul>
{{#if (or isSilverWebinar.registered (or isSilverWebinar.attendedLive (or isSilverWebinar.attendedReply)))}}
  <div class="hr-divider m-y">
    <h3 class="hr-divider-content hr-divider-heading">Silver Webinar Status&nbsp;</h3>
  </div>

  {{#if isSilverWebinar.registered}}
    <span class="label label-info">Registered</span>
  {{/if}}

  {{#if isSilverWebinar.attendedLive}}
    <span class="label label-success"> | Attended Live</span>
  {{/if}}

  {{#if isSilverWebinar.attendedReply}}
    <span class="label label-warning"> | Attended Reply</span>
  {{/if}}

{{/if}}