<div class="table-full">
  <div class="hr-divider m-t">
    <h3 class="hr-divider-content hr-divider-heading">Email Logs</h3>
  </div>
  <div class="table-responsive">
    <table class="table" data-sort="table">
      <thead>
      <tr>
        <th class="header">ID</th>
        <th class="header">Subject</th>
        <th class="header">body</th>
        <th class="header">status</th>
      </tr>
      </thead>
      <tbody>
      {{#if emailLogs}}
        {{#each emailLogs as |email|}}
          <tr>
            <td>{{email.attributes.email-id}}</td>
            <td>{{email.attributes.subject}}</td>
            <td>{{{email.attributes.body}}}</td>
            <td>{{email.attributes.status}}</td>
          </tr>
        {{/each}}
      {{else}}
        <tr class="padding-200">
          <td colspan="4"> {{ember-spinner}}</td>
        </tr>
      {{/if}}
      </tbody>
    </table>
  </div>
</div>
{{yield}}
