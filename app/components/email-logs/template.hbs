<div class="table-full">
  <div class="hr-divider m-t">
    <h3 class="hr-divider-content hr-divider-heading">Email Logs</h3>
  </div>
  <div class="table-responsive">
    <table class="table" data-sort="table">
      <thead>
      <tr>
        <th class="header">ID</th>
        <th class="header">Subject</th>
        <th class="header">body</th>
        <th class="header">status</th>
      </tr>
      </thead>
      <tbody>
      {{#with emailLogs as |email|}}
        <tr>
          <td>{{email.email-id}}</td>
          <td>{{email.subject}}</td>
          <td>{{{email.body}}}</td>
          <td>{{email.status}}</td>
        </tr>
      {{/with}}
      </tbody>
    </table>
  </div>
</div>
{{yield}}
